<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="com.nastation.pm.bean.ProjectIssueSequence" table="t_project_issue_sequence"
		lazy="false">

		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>



		<property name="maxIdValue" type="int" column="max_id_value" />




		<many-to-one name="projectId" column="project_id"
			class="com.nastation.pm.bean.Project" cascade="save-update" />






	</class>



</hibernate-mapping>

 
