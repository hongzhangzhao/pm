<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="com.nastation.pm.bean.FilterParameterValue" table="t_filter_parameter_value"
		lazy="false">

		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>



		<property name="paramValue" type="text" column="param_value" />
		<property name="paramOrder" type="int" column="param_order" />


		<many-to-one name="filterId" column="filter_id"
			class="com.nastation.pm.bean.SearchRequest" cascade="save-update" />






	</class>



</hibernate-mapping>

 
